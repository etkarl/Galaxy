                  <article id="ui-chosen" class="mod-doc">
                    <header>
                        <h1 class="title">ui-chosen:可搜索的下拉框</h1>
                    </header>
                    <div class="content">
                        <section class="overview">
                            <h2 class="cell-title">介绍</h2>
                            <div class="cell-content">
                                类datalist控件，即可搜索的下拉框
                            </div>
                        </section>
                        <section class="how">
                            <h2 class="cell-title">如何使用</h2>
                            <div class="cell-content">
                                <pre class="sh_javascript">
$.use('ui-chosen',function(){
    $('...').chosen(options);
});</pre>
                            </div>
                        </section>
                        <section class="demo">
                            <div class="cell-content">
                                <a class="btn btn-large btn-danger" href="http://harvesthq.github.com/chosen/" title="演示demo">demo</a>
                            </div>
                        </section>
                        <section class="config">
                            <h2 class="cell-title">配置</h2>
                            <div class="cell-content">
                                <table class="table table-bordered table-striped">
                                    <thead>
                                        <tr>
                                            <th>参数名</th>
                                            <th>说明</th>
                                            <th>类型</th>
                                            <th>default</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <tr>
                                            <td><em>no_results_text</em></td>
                                            <td>搜索没有结果时的文案</td>
                                            <td>String</td>
                                            <td>'No results matched'</td>
                                        </tr>
                                        <tr>
                                            <td><em>disable_search</em></td>
                                            <td>是否禁止搜索</td>
                                            <td>Boolean</td>
                                            <td>false</td>
                                        </tr>
                                        <tr>
                                            <td><em>enable_split_word_search</em></td>
                                            <td>是否允许分词搜索</td>
                                            <td>Boolean</td>
                                            <td>true</td>
                                        </tr>
                                        <tr>
                                            <td><em>allow_single_deselect</em></td>
                                            <td>是否允许撤销选择</td>
                                            <td>Boolean</td>
                                            <td>true</td>
                                        </tr>
                                        <tr>
                                            <td><em>max_selected_options</em></td>
                                            <td>最大选择个数</strong></td>
                                            <td>Number</td>
                                            <td>Infinity</td>
                                        </tr>
                                        <tr>
                                            <td><em>single_backstroke_delete</em></td>
                                            <td></td>
                                            <td>Boolean</td>
                                            <td>false</td>
                                        </tr>
                                    </tbody>
                                </table>
                            </div>
                        </section>
                    </div>
                  </article>